{
  "name": "deku-pokemon",
  "version": "1.0.1",
  "description": "Pokemon component for deku",
  "license": "MIT",
  "repository": "gillstrom/deku-pokemon",
  "author": {
    "name": "Andreas GillstrÃ¶m",
    "email": "andreasgillstrom@gmail.com",
    "url": "github.com/gillstrom"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "browserify example/index.js -o example/build.js -dv",
    "prepublish": "babel  --presets es2015,react index.js --out-file dist.js",
    "test": "xo index.js && ava"
  },
  "main": "dist.js",
  "files": [
    "dist.js"
  ],
  "keywords": [
    "component",
    "deku",
    "dekujs",
    "pokemon"
  ],
  "dependencies": {
    "classnames": "^2.2.1"
  },
  "devDependencies": {
    "assert-element": "^0.3.1",
    "ava": "*",
    "babel-cli": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "component-mock": "0.0.2",
    "deku": "^2.0.0-rc4",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.3.1",
    "xo": "*"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/no-unknown-property": 0,
      "react/prop-types": 0,
      "quote-props": 0
    }
  }
}
